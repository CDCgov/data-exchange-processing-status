# RabbitMQ Configuration
rabbitmq_host="{value}"
rabbitmq_user="{value}"
rabbitmq_password="{value}"
rabbitmq_vhost="/"
rabbitmq_exchange_name="{value}"
rabbitmq_queue_name="{value}"

# Target Service Configuration
# The target_service can either be 'aws' or 'azure'
# The use_queue is true for queues AND false for topics
target_service="aws"
use_queue="true"


# AWS Configuration (if using aws)
aws_access_key_id="{value}"
aws_secret_access_key="{value}"
aws_region="{value}"
aws_queue_url="{value}"
aws_topic_arn="{value}"

# Azure Service Bus Configuration (if using Azure)
azure_service_bus_connection_str="{value}"
azure_queue_name="{value}"
azure_topic_name="{value}"
