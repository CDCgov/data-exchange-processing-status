{
  "$schema": "dab.draft.schema.json",
  "data-source": {
    "database-type": "cosmosdb_nosql",
    "connection-string": "{{place here}}",
    "options": {
      "database": "ProcessingStatus",
      "schema": "schema.gql"
    }
  },
  "runtime": {
    "rest": {
      "enabled": false,
      "path": "/api",
      "request-body-strict": true
    },
    "graphql": {
      "enabled": true,
      "path": "/graphql",
      "allow-introspection": true
    },
    "host": {
      "cors": {
        "origins": [],
        "allow-credentials": false
      },
      "authentication": {
        "provider": "StaticWebApps"
      },
      "mode": "development"
    }
  },
  "entities": {
    "Report": {
      "source": {
        "object": "Reports",
        "type": "table",
        "parameters": {},
        "key-fields": []
      },
      "graphql": {
        "enabled": true,
        "type": {
          "singular": "Report",
          "plural": "Reports"
        }
      },
      "rest": {
        "enabled": false
      },
      "permissions": [
        {
          "role": "anonymous",
          "actions": [
            {
              "action": "*"
            }
          ]
        }
      ]
    },
    "GetMetadataReports": {
      "source": {
        "object": "ProcessingStatus.test2",
        "type": "stored-procedure",
        "parameters": {}
      },
      "graphql": {
        "enabled": true,
        "operation": "query"
      },
      "rest": {
        "enabled": false
      },
      "permissions": [
        {
          "role": "anonymous",
          "actions": [ "execute" ]
        }
      ]
    }
  }
}