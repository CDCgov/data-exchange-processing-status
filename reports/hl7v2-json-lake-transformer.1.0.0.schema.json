{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$id": "https://github.com/cdcent/data-exchange-messages/reports/base",
    "title": "HL7v2 JSON Lake Transformer Report",
    "type": "object",
    "required": ["content_schema_name", "content_schema_version"],
    "properties": {
        "content_schema_name": {
            "type": "string",
            "const": "hl7v2-json-lake-transformer"
        },
        "content_schema_version": {
            "type": "string",
            "const": "1.0.0"
        },
        "configs": {
            "type": "array",
            "items": {
                "type": "string"
            }
        }
    }
}
